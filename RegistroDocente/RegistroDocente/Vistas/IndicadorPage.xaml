<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="RegistroDocente.Vistas.IndicadorPage"
             xmlns:vm="clr-namespace:RegistroDocente.ViewModels"
             xmlns:local="clr-namespace:RegistroDocente.Utils"
             Title="Indicador"
             Padding="8">
    <StackLayout>
        <Entry x:Name="txtTexto" />
        <Entry x:Name="txtValor" Text="5"/>
        <StackLayout Orientation="Horizontal">
            <Button Text="Guardar" x:Name="btnGuardar" Clicked="btnGuardar_Clicked" />
            <Button Text="Cancelar" x:Name="btnCancelar" Clicked="btnCancelar_Clicked" />
        </StackLayout>
    </StackLayout>
</ContentPage>